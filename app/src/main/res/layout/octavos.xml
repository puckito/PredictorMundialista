        android:layout_marginTop="32dp"
        android:background="@android:color/white"
        android:text="a1"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/b2"
        app:layout_constraintEnd_toStartOf="@+id/b1"
